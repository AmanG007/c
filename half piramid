// Print the number piramid
#include <stdio.h>

int main() {
    // Write C code here
    int i,j,n;
    printf("Enter any number:");
    scanf( "%d", &n);
    
    for(i=1; i<=n; i++){
        for(j=1; j<=i; j++){
             printf("%d", j);
        } 
        printf("\n");
    }

    return 0;
}
